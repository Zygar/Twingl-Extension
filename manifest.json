{
  "manifest_version": 2,

  "name": "Twingl",
  "description": "Development instance of the Twingl extension.",
  "version": "0.1",

  "permissions": ["<all_urls>"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["annotator.min.css"],
      "js": ["jquery-2.0.3.min.js", "annotator.min.js", "twingl.js"]
    }
  ]
}